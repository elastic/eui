    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.3.0",
