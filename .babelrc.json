{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "> 5%",
          "Safari 7"
        ]
      },
      "useBuiltIns": true
    }],
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-async-generator-functions",
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}
